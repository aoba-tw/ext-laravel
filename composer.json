{
    "name": "aoba-tw/ext-laravel",
    "type": "library",
    "description": "Laravel Extensions",
    "keywords": ["extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yuchen Chang",
            "email": "phiricen@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "laravel/framework": "^8.75"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "AobaTw\\ExtLaravel\\App\\": "app"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
